<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane layoutY="20.0" prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: #577280;">
         <children>
            <Label layoutX="74.0" layoutY="12.0" text="Nouveau propriétaire" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <ImageView fx:id="addIcon" fitHeight="42.0" fitWidth="44.0" layoutX="14.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </Pane>
      <VBox layoutX="31.0" layoutY="91.0" prefHeight="218.0" prefWidth="162.0" spacing="15.0">
         <children>
            <TextField fx:id="nomTextField" promptText="Nom" />
            <TextField fx:id="prenomTextField" promptText="prénom" />
            <TextField fx:id="adressTextField" promptText="Adresse" />
            <TextField fx:id="emailTextField" promptText="E-mail" />
            <TextField fx:id="phoneTextfield" promptText="Téléphone" />
         </children>
      </VBox>
      <Button layoutX="171.0" layoutY="335.0" mnemonicParsing="false" onAction="#creerProp" text="Créer" />
   </children>
</AnchorPane>
