<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="562.0" prefWidth="293.0" spacing="5.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.modifierAppartController">
   <padding>
      <Insets left="20.0" top="40.0" />
   </padding>
   <children>
      <HBox prefHeight="24.0" prefWidth="260.0" spacing="10.0">
         <children>
            <VBox alignment="TOP_RIGHT" prefHeight="200.0" prefWidth="100.0" spacing="18.0" style="-fx-alignment: right;">
               <children>
                  <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="Transaction :" />
                  <Label text="Date d'ajout : " />
                  <Label text="TypeBien :">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label text="Adresse : " />
                  <Label text="Wilaya :" />
                  <Label text="Superficie :" />
                  <Label text="Prix Initial:" textFill="#cd860d">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <VBox fillWidth="false" prefHeight="246.0" prefWidth="126.0" spacing="10.0">
               <children>
                  <TextField fx:id="transactionTextField" prefHeight="18.0" prefWidth="151.0" />
                  <TextField fx:id="dateajoutTextField" />
                  <TextField fx:id="typeBienTextField" prefHeight="18.0" prefWidth="126.0" />
                  <TextField fx:id="adresseTextField" />
                  <TextField fx:id="wilayaTextField" />
                  <TextField fx:id="superficieTextField" />
                  <TextField fx:id="prixTextField" />
                  <ToggleButton fx:id="negociableToggleButton" mnemonicParsing="false" text="Negociable" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Label />
      <Button fx:id="modificationButton" mnemonicParsing="false" prefWidth="240.0" text="Modifier" />
      <Label fx:id="modifieSuccess" contentDisplay="CENTER" text="Label" />
   </children>
</VBox>
